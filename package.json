{
  "name": "workout-tracker-api",
  "version": "1.0.0",
  "description": "A workout tracker api written in node.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:swagger": "tsx ./src/app/infrastructure/swagger/swagger.ts",
    "db:up": "npm run db-migrate up",
    "db:down": "npm run db-migrate down",
    "start": "npm run db:up && npm run build:swagger && tsx ./src/app.ts",
    "dev": "tsx watch ./src/app.ts",
    "db-migrate": "node node_modules/db-migrate/bin/db-migrate",
    "lint:fix": "eslint --fix .",
    "prepare": "husky"
  },
  "keywords": [
    "node",
    "javascript",
    "api"
  ],
  "author": "adriangubedev@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@types/swagger-ui-express": "^4.1.6",
    "bcrypt": "^5.1.1",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "dotenv": "^16.4.5",
    "express": "^4.20.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.12.0",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.1",
    "tsx": "^4.19.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.5",
    "@types/pg": "^8.11.10",
    "eslint": "^9.11.0",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.6.0"
  },
  "lint-staged": {
    "*{js,ts}": "eslint --fix"
  }
}
